apiVersion: v1
clusters:
  - cluster:
      certificate-authority-data: ${ca_pem}
      server: https://${server_host}:443
    name: default
contexts:
  - context:
      cluster: default
      user: default
    name: default
current-context: default
kind: Config
preferences: {}
users:
  - name: default
    user:
      client-certificate-data: ${client_cert_pem}
      client-key-data: ${client_key_pem}
